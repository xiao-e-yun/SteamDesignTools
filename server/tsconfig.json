{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2020",
        "outDir":"../build/server/",
        "strict":true,
        "sourceMap": true,
        "moduleResolution":"Node",
        "esModuleInterop": true,
        "baseUrl": "./",
        "typeRoots": [
            "node_modules/@types",
        ],
        "paths": {
            "VS/*": [
                "../types/*"
            ]
        }
    },
    "include": [
        "*.ts",
        "**/*.ts"
    ]
}